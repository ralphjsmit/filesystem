{
  "name": "ralphjsmit/stubs",
  "description": "A package to simplify complex stub workflows.",
  "keywords": [
    "ralphjsmit",
    "laravel",
    "stubs"
  ],
  "homepage": "https://github.com/ralphjsmit/stubs",
  "license": "MIT",
  "authors": [
    {
      "name": "Ralph J. Smit",
      "email": "rjs@ralphjsmit.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/contracts": "^8.73",
    "spatie/laravel-package-tools": "^1.9.2",
    "symfony/finder": "^5.4"
  },
  "require-dev": {
    "nunomaduro/collision": "^5.10",
    "orchestra/testbench": "^6.22",
    "pestphp/pest": "^1.21",
    "pestphp/pest-plugin-laravel": "^1.1",
    "phpunit/phpunit": "^9.5",
    "spatie/laravel-ray": "^1.26"
  },
  "autoload": {
    "psr-4": {
      "RalphJSmit\\Stubs\\": "src",
      "RalphJSmit\\Stubs\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RalphJSmit\\Stubs\\Tests\\": "tests"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "RalphJSmit\\Stubs\\StubsServiceProvider"
      ],
      "aliases": {
        "Stubs": "RalphJSmit\\Stubs\\Facades\\Stubs"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
